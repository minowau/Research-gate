{
  "version": 1,
  "canisters": {
    "paperStorage": {
      "type": "motoko",
      "main": "src/paperStorage/main.mo"
    },
    "daoSystem": {
      "type": "motoko", 
      "main": "src/daoSystem/main.mo"
    },
    "aiChatbot": {
      "type": "motoko",
      "main": "src/aiChatbot/main.mo"
    },
    "deresnet_frontend": {
      "type": "assets",
      "source": ["dist/"]
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env"
}